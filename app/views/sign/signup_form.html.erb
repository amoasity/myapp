<div class="sign_form">
    <%= form_with url: "/signup", local: true do |f| %>
        <div class="sign_name">
            <%= f.label :name, "name" %>
            <p><%= f.text_field :name %></p>
        </div>
        <div class="sign_email">
            <%= f.label :email, "email" %>
            <p><%= f.email_field :email %></p>
        </div>
        <div class="sign_password">
            <%= f.label :password_digest, "password" %>
            <p><%= f.password_field :password_digest %></p>
        </div>
        <div class="sign_btn">
            <%= f.submit "singup" %>
        </div>
    <% end %>
    <%= link_to("signin", "/") %>
</div>